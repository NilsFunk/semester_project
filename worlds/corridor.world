<?xml version="1.0" ?>
<sdf version="1.4">
  <world name="default">
    <include>
      <uri>model://ground_plane</uri>
      <color>0 0 0 </color>
    </include>
    <include>
      <uri>model://sun</uri>
    </include>

    <!--light name='sun' type='directional'>
      <cast_shadows>1</cast_shadows>
