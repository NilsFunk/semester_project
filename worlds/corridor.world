    <!-- long wall left -->
    <model name="wall_left" static="true">
      <pose>2.0 0 0 0 0 0</pose>
      <static>true</static>
      <link name="wall_body">
        <visual name="wall_visual">
          <geometry>
            <box>
                <size>.4 30 15</size>
            </box>
          </geometry>
          <material>
            <script>
              <name>Gazebo/Grey</name>
              <uri>file://media/materials/scripts/gazebo.material</uri>
            </script>
          </material>
        </visual>
        <collision name="wall_collision">
          <geometry>
            <box>
              <size>.4 30 15</size>
            </box>
          </geometry>
        </collision>
      </link>
    </model>

